5. SET @test_date = 'xxxx-xx-xx';

SELECT 
    YEAR(@test_date) AS Year,
    CASE 
        WHEN (YEAR(@test_date) % 4 = 0 AND YEAR(@test_date) % 100 != 0) 
             OR (YEAR(@test_date) % 400 = 0) THEN 'Leap Year'
        ELSE 'Not a Leap Year'
    END AS Status;

